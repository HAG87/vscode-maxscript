{
	"name": "language-maxscript",
	"displayName": "Language MaxScript",
	"description": "Autodesk 3ds Max MaxScript language support",
	"version": "1.50.0",
	"publisher": "atelierbump",
	"icon": "mxs.png",
	"homepage": "https://github.com/HAG87/vscode-maxscript",
	"repository": {
		"type": "git",
		"url": "https://github.com/HAG87/vscode-maxscript"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Programming Languages"
	],
	"license": "MIT",
	"keywords": [
		"MaxScript"
	],
	"activationEvents": [
		"onLanguage:maxscript",
		"onCommand:mxs.minify.file",
		"onCommand:mxs.minify.files"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "maxscript",
				"aliases": [
					"MaxScript",
					"maxscript",
					"mxs"
				],
				"extensions": [
					".ms",
					".mcr"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "maxscript",
				"scopeName": "source.mxs",
				"path": "./syntaxes/maxscript.tmLanguage.json"
			}
		],
		"semanticTokenScopes": [
			{
				"language": "maxscript",
				"scopes": {
					"class": [
						"support.class.mxs"
					],
					"classclass.defaultLibrary": [
						"support.class.mxs"
					],
					"function.defaultLibrary": [
						"support.function.mxs"
					],
					"interface": [
						"support.type.interface.mxs"
					],
					"namespace": [
						"support.superclass.mxs"
					],
					"struct": [
						"storage.type.struct.mxs"
					],
					"type.defaultLibrary": [
						"support.type.primitive.mxs"
					],
					"type": [
						"support.type.primitive.mxs"
					],
					"variable": [
						"support.variable.system.mxs"
					],
					"variable.other.readwrite": [
						"support.variable.system.mxs"
					]
				}
			}
		],
		"commands": [
			{
				"command": "mxs.help",
				"title": "Code reference...",
				"category": "MaxScript"
			},
			{
				"command": "mxs.minify",
				"title": "Minify open document",
				"category": "MaxScript"
			},
			{
				"command": "mxs.minify.file",
				"title": "Minify file",
				"category": "MaxScript"
			},
			{
				"command": "mxs.minify.files",
				"title": "Minify files...",
				"category": "MaxScript"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceExtname == .ms || resourceExtname == .mcr",
					"command": "mxs.minify.file",
					"group": "1_modification@1"
				}
			],
			"editor/context": [
				{
					"when": "editorHasSelection && editorTextFocus && resourceLangId == maxscript",
					"command": "mxs.help",
					"group": "navigation@1"
				}
			],
			"commandPalette": [
				{
					"when": "editorLangId  == maxscript",
					"command": "mxs.minify"
				},
				{
					"command": "mxs.minify.files"
				}
			]
		},
		"snippets": [
			{
				"language": "maxscript",
				"path": "./snippets/maxscript.json"
			}
		],
		"configurationDefaults": {
			"[maxscript]": {
				"editor.semanticHighlighting.enabled": true
			}
		},
		"configuration": {
			"title": "Language MaxScript",
			"type": "object",
			"properties": {
				"maxscript.gotosymbol": {
					"type": "boolean",
					"default": true,
					"description": "Enable Document Symbols and Diagnostics."
				},
				"maxscript.gotodefinition": {
					"type": "boolean",
					"default": true,
					"description": "Enable Document Definitions."
				},
				"maxscript.diagnostics": {
					"type": "boolean",
					"default": true,
					"description": "Enable Document diagnostics"
				},
				"maxscript.completions": {
					"type": "boolean",
					"default": true,
					"description": "Enable suggest completions"
				},
				"maxscript.semantics": {
					"type": "boolean",
					"default": true,
					"description": "Enable Semantic highlight"
				},
				"maxscript.helpprovider": {
					"type": "string",
					"default": "http://help.autodesk.com/view/3DSMAX/2018/ENU/",
					"description": "maxScript help source"
				},
				"maxscript.minprefix": {
					"type": "string",
					"default": "min_",
					"description": "Prefix for code minify output"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"test": "node ./out/test/runTest.js",
		"lint": "eslint -c .eslintrc.js --ext .ts ./",
		"fix": "eslint --fix -c .eslintrc.js --ext .ts ./"
	},
	"devDependencies": {
		"@types/eslint": "^6.8.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/moo": "^0.5.3",
		"@types/nearley": "^2.11.1",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.33.0",
		"@typescript-eslint/parser": "^2.33.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.9.2",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"ast-monkey-traverse-with-lookahead": "^1.1.2",
		"ast-monkey-util": "^1.1.5",
		"moo": "^0.5.1",
		"nearley": "^2.19.3",
		"object-path": "^0.11.4"
	},
	"__metadata": {
		"id": "7f2edb68-8e0f-4a8c-9380-24d100650a02",
		"publisherDisplayName": "Atelier Bump",
		"publisherId": "9039a6b2-5163-4caa-8366-6f65e2d75d03"
	}
}
