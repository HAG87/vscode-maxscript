{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "maxscript",
	"fileTypes": [ "ms", "mse", "mcr" ],
	"foldingStartMarker": "(-{2})?\\(\\s*$",
	"foldingStopMarker": "^\\s*(-{2})?\\s*\\)",

	"patterns": [
		{ "include": "#braces"},
		{ "include": "#variable"},
		{ "include": "#comment" },
		{ "include": "#keywords" },
		{ "include": "#strings" },
		{ "include": "#numeric" },
		{ "include": "#constant" },
		{ "include": "#storage" },
		{ "include": "#meta" },
		{ "include": "#support"},
		{ "include": "#struct-class-interface"}
	],
	"repository": {
		"braces":{
			"patterns": [
				{
					"name": "punctuation.section.brackets.begin",
					"match": "\\["
				},
				{
					"name": "punctuation.section.brackets.end",
					"match": "\\]"
				},
				{
					"name": "punctuation.section.braces.begin",
					"match": "\\{"
				},
				{
					"name": "punctuation.section.braces.end",
					"match": "\\}"
				},
				{
					"name": "punctuation.section.block.begin",
					"match": "\\("
				},
				{
					"name": "punctuation.section.block.end",
					"match": "\\)"
				}
			]
		},
		"comment": {
			"patterns": [
				{
					"name": "comment.line.maxscript",
					"begin":"--",
					"end":"$",
					"captures": {
						"1":{"name":"punctuation.definition.comment.maxscript"}
					}
				},
				{
					"name":"comment.block.maxscript",
					"begin": "/\\*",
					"end":"\\*/",
					"captures":{
						"1":{"name":"punctuation.definition.comment.maxscript"}
					}
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name":"keyword.operator.maxscript",
					"match": "(?i)(?:(\\+=?|\\*=?|-=?|\\/=?|\\^|!?=|={1,2}|<=?|>=?))"
				},
				{
					"name":"keyword.operator.linebreak.maxscript",
					"match": "(?i)(\\\\)"
				},
				{
					"name": "keyword.operator.statement.maxscript",
					"match": ";"
				},
				{
					"name": "keyword.operator.delimiter.maxscript",
					"match": "[,\\.](?!\\d)"
				},
				{
					"name":"keyword.control.assign.maxscript",
					"match": "(?<!:):(?=[^:]|:{2}[^:])"
				},
				{
					"name":"keyword.operator.word.maxscript",
					"match": "(?i)(?<![.$])\\b(and|at|as|by|in|not|or|to)\\b"
				},
				{
					"name": "keyword.control.maxscript",
					"match": "(?i)\\b(case|catch|collect|continue|do|of|else|exit|for|if|return|set|then|throw|try|undo|when|where|while|with|dontcollect)\\b"
				},
				{
					"name":  "keyword.other.maxscript",
					"match": "(?i)\\b(max|animate|from)\\b"
				},
				{
					"name":"keyword.operator.definition.byref.maxscript",
					"match": "(&)(?=\\w)"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name":"string.localized.maxscript",
					"begin": "~",
					"end": "~",
					"captures": {
						"0": {"name":"punctuation.definition.localized.maxscript"}
					},
					"patterns": [
						{
							"name":  "constant.character.escape.maxscript",
							"match": "\\\\(\\*|\\?|\\\\|\\')"
						}
					]
				},
				{
					"name": "string.quoted.double.maxscript",
					"begin": "(?<!@)\"",
					"end": "\"",
					"beginCaptures": {
						"0": {"name":"string.begin.maxscript" }
					},
					"endCaptures": {
						"0": {"name":"string.end.maxscript"}
					},
					"patterns": [
						{
							"name": "constant.character.escape.maxscript",
							"match": "\\\\(\"|n|r|t|\\*|\\?|\\\\|%|x[A-Fa-f0-9]+)"
						}
					]
				},
				{
				"name":  "string.quoted.double.verbatim.maxscript",
				"begin": "(@)(\")",
				"end":   "\"",
				"beginCaptures": {
					"1": {"name": "string.verbatim.operator.maxscript" },
					"2": {"name": "string.begin.maxscript"}
				},
				"endCaptures":   {"0": {"name": "string.end.maxscript" }}
				},
				{
					"name":  "string.quoted.single.maxscript",
					"begin": "'",
					"end":   "'",
					"beginCaptures": {
						"0": {"name":"string.begin.maxscript" }
					},
					"endCaptures": {
						"0": {"name": "string.end.maxscript"}
					},
					"patterns": [{
						"name":  "constant.character.escape.maxscript",
						"match": "\\\\(\\*|\\?|\\\\|\\')"
					}]

				},
				{
					"name":  "string.other.name.maxscript",
					"match": "(#)(?:('[^']*')|(\\w+))",
					"captures": {
						"1": {"name": "string.other.name.begin.maxscript"},
						"2": {"name": "string.other.name.quoted.maxscript"},
						"3": {"name": "string.other.name.unquoted.maxscript"}
					}
				}
			]
		},
		"meta": {
			"patterns":[
				{
					"name":"meta.definition.struct.maxscript",
					"match": "(?i)\\b(struct)\\b\\s+(\\w+)",
					"captures": {
						"1":{"name":"entity.type.struct.maxscript"},
						"2":{"name":"entity.name.struct.maxscript"}
					}
				},
				{
					"name":"meta.definition.function.maxscript",
					"begin":"(?i)\\b((mapped\\s+)?fn|function)\\s+(\\w+)",
					"end":"=",
					"beginCaptures": {
						"1": {"name": "entity.modifier.function.maxscript"},
						"2": {"name": "entity.type.function.maxscript"},
						"3": {"name": "entity.name.function.maxscript"}
					},
					"endCaptures": {
						"0": {"name": "keyword.operator.maxscript"}
					},
					"patterns": [
						{"include": "#variable"},
						{"include": "$self"}
					]
				},
				{
					"name":"meta.definition.event.maxscript",
					"begin": "(?im)^\\s*\\b(on)\\b",
					"end": "(?i)\\b(do|return)\\b",
					"beginCaptures": {
						"1":{"name":"entity.type.event.maxscript"}
					},
					"endCaptures": {
						"1": {"name": "keyword.control.maxscript"}
					},
					"patterns": [
						{
							"match": "(?i)(?<=on)\\s+(\\b\\w+)\\s+(?=\\b(do|return)\\b)",
							"captures": {
								"1":{"name":"entity.name.event.maxscript"}
							}
						},
						{
							"match":"(?i)(?<=on)\\s+(\\b\\w+)\\s+(\\b\\w+)\\s+(?=\\b(do|return)\\b)",
							"captures":{
								"1":{"name":"entity.target.event.maxscript"},
								"2":{"name":"entity.name.event.maxscript"}
							}
						},
						{
							"match":"(?i)(?<=on)\\s+(\\w+\\b)\\s+(\\w+\\b)((?:\\s+(?!do|return)\\w+)+)",
							"captures":{
								"1":{"name":"entity.target.event.maxscript"},
								"2":{"name":"entity.name.event.maxscript"},
								"3":{"name":"entity.argument.event.maxscript"}
							}
						}
					]
				},
				{
					"name": "meta.definition.utility.maxscript",
					"match": "(?i)\\b(macroscript|parameters|plugin|rcmenu|rollout|tool|utility)\\b\\s+(\\w+)",
					"captures": {
						"1":{"name":"entity.type.utility.maxscript"},
						"2":{"name":"entity.name.utility.maxscript"}
					}
				}

			]
		},
		"variable":{
			"patterns": [
				{
					"name":"variable.property.maxscript",
					"match": "(?<=\\.)([0-9]*[A-Za-z_]+\\w*)\\b"
				},
				{
					"match": "\\b([0-9]*[A-Za-z_]+\\w*)(?:[:])\\s*",
					"captures": {
						"1":{"name":"variable.parameter.maxscript"}
					}
				},
				{
					"name":"variable.language.maxscript",
					"match": "(?i)\\b(this)\\b"
				},
				{
					"name": "variable.other.maxscript",
					"comment": "Matches keyword assignment in function calls and definitions (e.g., fn add flag:#optional)",
					"match":   "\\b(\\w+)\\b\\s*(?=:[^:])",
					"captures": {
						"1":{"name":"variable.other.name.maxscript"}
					}
				},
				{
					"name": "variable.definition.global.maxscript",
					"match": "(:{2})(\\w+)"
				}
			]
		},
		"constant":{
			"patterns": [
				{
					"name": "constant.numeric.integer.maxscript",
					"match": "(?<!\\.)\\b[+-]?\\d+[lLpP]?(?!\\.)\\b"
				},
				{
					"name":"constant.numeric.float.maxscript",
					"match": "\\b\\d+\\.\\d+([EeDd][+-]?\\d+)?"
				},
				{
					"name":"constant.numeric.float.no-trailing.maxscript",
					"match": "\\b(\\d+)(\\.)(?!\\w)"
				},
				{
					"name":  "constant.numeric.float.no-leading.maxscript",
					"match": "(?<!\\w)(\\.\\d+([EeDd][+-]?\\d+)?)"
				},
				{
					"name":  "constant.numeric.time.maxscript",
					"match": "(?<!\\w|\\.)(?:\\d*\\.\\d+|\\d+\\.?\\d*)[msft](?:(?:\\d+|\\d*\\.\\d*)[msft])*"
				},
				{
					"name":  "constant.numeric.time.smpte.maxscript",
					"match": "(?<!\\w)\\d+:\\d*\\.\\d*(?!\\w)"
				},
				{
					"name":  "constant.numeric.time.normalised.maxscript",
					"match": "(?<!\\w|\\.)(?:\\d*\\.\\d+|\\d+\\.?\\d*)n(?!\\w)"
				},
				{
					"name":  "constant.language.boolean.maxscript",
					"match": "(?i)\\b(true|on|false|off)\\b"
				},
				{
					"name":  "constant.language.math.maxscript",
					"match": "(?i)\\b(pi|e)\\b"
				},
				{
					"name":  "constant.language.colour.maxscript",
					"match": "(?i)\\b(red|green|blue|white|black|orange|yellow|brown|gray)\\b"
				},
				{
					"name":  "constant.language.axis.maxscript",
					"match": "(?i)\\b([xyz]_axis)\\b"
				},
				{
					"name":  "constant.language.null.maxscript",
					"match": "(?i)\\b(undefined|unsupplied|silentvalue|ok)\\b"
				},
				{
					"name": "constant.other.hex.maxscript",
					"match": "\\b(0x\\w+)\\b"
				},
				{
					"name":"constant.language.pathname.maxscript",
					"begin": "\\$",
					"end": "$",
					"patterns": [{
						"match": "[^ \t\r\n]+"
					}]
				}
			]
		},
		"storage":{
			"patterns": [
				{
					"name": "storage.modifier.maxscript",
					"match": "(?i)\\b(about|coordsys|global|local|private|public|persistent)\\b"
				},
				{
					"name": "storage.type.array.maxscript",
					"begin": "(#)\\s*(\\()",
					"end": "\\)",
					"beginCaptures":{
						"1": {"name": "keyword.definition.array.begin.maxscript"},
						"2": {"name": "punctuation.section.block.begin"}
					},
					"endCaptures":{
						"0": {"name": "punctuation.section.block.end"}
					},
					"patterns": [
						{"include": "$self"},
						{"include": "#braces"}
					]
				},
				{
					"name": "storage.type.bitarray.maxscript",
					"patterns": [{"include": "$self"}],
					"begin": "(#)\\s*(\\{)",
					"end": "\\}",
					"beginCaptures":{
						"1": {"name": "keyword.definition.bit.array.begin.maxscript"},
						"2": {"name": "punctuation.section.braces.begin"}
					},
					"endCaptures":{
						"0": {"name": "punctuation.section.braces.end"}
					}
				},
				{
					"name":  "storage.type.bit-range.maxscript",
					"match": "(?<!\\$)\\b(\\d+)(\\.{2})(\\d+)\\b",
					"captures": {
						"1": {"name": "storage.type.bit-range.begin.maxscript"},
						"2": {"name": "storage.type.bit-range.middle.maxscript"},
						"3": {"name": "storage.type.bit-range.end.maxscript"}
					}
				},
				{
					"name": "storage.type.primitive.maxscript",
					"match": "(?i)\\b(AngleAxis|Array|BigMatrix|BigMatrixRowArray|BinStream|BitArray|Bitmap|Box2|Box3|color|DataPair|Dictionary|Double|EulerAngles|FileStream|Float|HKey|Integer|Integer64|IntegerPtr|Matrix3|MAXKey|mesh|Name|PathName|Point2|Point3|Point4|Quat|Ray|String|StringStream|Time|Timer|TriMesh)\\b"
				}
			]
		},
		"struct-class-interface":{
			"patterns": [
				{
					"name": "support.type.StructDef.maxscript",
					"match": "(?i)\\b(AMG|AssertReporter|AttachCtrl|BezierDefaultParams|CAT_ParamBlock|CAT_UIItem|CAT_UIItem2|CINCfg|CombustionWorkSpace|DOF|DontShowAgainDialog|Filters|ILayerManager|IMouseDragger|KeyVal_struct|LE|LinkCtrl|ListCtrl|ListViewOps|MCGParameterOps|Main_Ribbon|MatEditor|NURMS_CageSelected_ColorSwatch|NURMS_Cage_ColorSwatch|NodeExposureInterface|ObjectPaintStruct|PolyBoostStruct|PolyToolsUIstruct|ProceduralContentOps|PxRolloutInfo_struct|RElement|RTT_methods_struct|Ribbon_Modeling|Snaps|SortedArray_Struct|TCBDefaultParams|TreeViewWraps|VFB_ViewportInfo|VFB_ViewportInfoEntry|VFB_methods_struct|VLD|VRSceneExportInfo|WAVsound|assetBrowser|autoBackup|biped|bit|boolObj|cacheOps|callbacks|cameraFOV|crowds|cui|custAttributes|cwschannel|cwscomposite|cwskey|cwsobject|cwsoperator|delegates|displayColor|dotNet|envEffectsDialog|fileProperties|flexOps|globalMotionClipOps|globalVars|gw|hideByCategory|ik|internet|keyboard|lightCreationToolStr|listview|macros|mapPaths|mcrUtils|meditUtilities|meshOps|meshop|mocap|modPanel|mouse|mtlBrowser|options|patch|patchOps|pathConfig|pathConfig|persistents|pluginPaths|pointCacheMan|polyOps|polyop|preferences|pxNodeKeys|refs|registry|renderSceneDialog|renderers|rolloutCreator|scanlineRender|schematicView|sessionPaths|setKey|skinOps|snapMode|splineOps|surfaceArriveBhvr|sxmlIO|symbolicPaths|sysInfo|systemTools|tabbedDialogs|terrainOps|theHold|themixer|timeConfiguration|toolMode|trackbar|units|uvwManipUtils|vfields|viewport|windows|xrefPaths|xrefs)\\b"
				},
				{
					"name": "support.class.maxscript",
					"match": "(?i)\\b(Editable_Poly|Editable_Mesh|AlphaMap|Anchor|ArbBone|AreaAtmosphere|Bend|Bevel|Birth|BirthGrid|BirthStream|BitmapPagerData|Bitmaptexture|Blackman|Blend|BlendFragment|BlendMap|BlendRenderElement|BlendedBoxMap|Blendfilter|Blinn|BlitFragment|Blizzard|BlobMesh|Block|BlurWind|Bomb|Bombbinding|Bone|BoneGeometry|Bones|Boolean2|Boolean3|Box|BoxGizmo|Bricks|CFractal|CIN|CMB|CProxy|CScatter|CTexmap|CVrWarp|Cache|CacheFile|Caddy|CamPerspCorrect|CamPoint|CameraMap|CameraMapSpaceWarp|Capsule|Casement|Cellular|Chamfer|ChamferBox|ChamferCyl|Character|CharacterAssembly|Checker|Circle|ClayFragment|ClipAssigner|ClipState|Cloth|CogControl|Collision|ColorMap|Combustion|Compass|CompleteMap|Component|CompositeTexturemap|Condition|Cone|Conform|ConformSpaceWarp|Connect|Container|ControlContainer|Convert|Cookie|Crease|CreaseSet|CrosFade|CrossSection|Crowd|CrowdAssignment|CrowdState|CrowdTeam|CrowdTransition|CylGizmo|Cylinder|D6Joint|DAEEXP|DAEIMP|DDS|Damper|Daylight|DaylightAssemblyHead|Deflector|Deflectorbinding|Delegate|DeleteMesh|DeleteParticles|DeletePatch|DeleteSplineModifier|Dent|DepthFragment|DialogMonitor|Diffuse|DigitData|Directionallight|Discretizator|Displace|Displacebinding|DisplayData|DisplayParticles|Donut|DoubleSided|Drag|DragMod|Dummy|DummyRadMapClass|DynBuilding|Ease|Egg|Ellipse|EmptyModifier|Emulator|Event|ExportHTR|ExposeTm|Extrude|FFDBox|FFDCyl|FPToolbar|Fade|Fixed|Flex|Flow|FltImport|Fog|FogHelper|Foliage|FoliagetextureMap|Footsteps|Force|FragmentGraph|Freecamera|GIF|GameExporter|Gengon|GeoSphere|GhostFrameFragment|GhostRenderFragment|GizmoFragment|Gradient|GradientEdge|Gravitybinding|Greeble|HDRI|HairEffect|HairMod|HalfRound|Hedra|HeightMap|Helix|Hose|HubObject|HubTrans|IFL|IKChainControl|IKControl|IKHISolver|IKLimb|IKTargTrans|IKTarget|IdleAreaObj|IlluminanceFragment|ImportHTR|ImportTRC|Ink|InkNPaint|Inline|InputCustom|InputStandard|InventorImport|JPEG|KeyValueUtility|LOD|Lathe|Lattice|LayerFloat|LayerInfo|LayerMatrix3|LayerRoot|LayerTransform|LayerWeights|LightMap|LightMeter|Lighting|LightingMap|LinearShape|Lines|LinkBlockInstance|LinkComposite|LinkCompositeshape|LinkLeaf|LinkLeafshape|LinkOriginPtHelper|LinkTimeControl|LockedMapWrapper|LockedObjectWrapper|Loft|LoftObject|Luminaire|MAXScript|MEditBkgnd|MPG|MSCollision|MapMtlClipboard|MapScaler|MapScalerOSM|MapScalerSpaceWarp|Marble|Mask|MasterBlock|MaterialByElement|MaterialPreviewer|Materialmodifier|MatteRenderElement|MatteShadow|MaxMotionClipMaster|Measure|Melt|MeshProjIntersect|Mesher|MetalShader|Metronome|MinMaxAvg|Mix|MocapLayer|Morph|Morpher|MorpherMaterial|Motor|MotorMod|MultiMatteElement|MultiPainter|MultiRes|MultiTile|Multimaterial|MusclePatch|NPRFragment|NURBSCurveshape|NURBSSurf|NavInfo|Ngon|NoMaterial|NoTexture|NodeMonitor|NodeTransformMonitor|Noise|Noisemodifier|Normalmodifier|NormalsMap|Notes|NotesReferenceTarget|ObjAssoc|ObjExp|ObjImp|ObjectParameter|OilTank|OldBoolean|OldVertexPaint|Omnilight|OneClick|OpenEXR|OpenFltExport|OpenSubdiv|OutputCustom|OutputNew|OutputStandard|OutputTest|OverlayFragment|PB2Parameter|PBomb|PBombMod|PCloud|PDAlpha|PFActionListPool|PFArrow|PFBoxMeshWrapper|PFCapsuleMeshWrapper|PFConvexMeshWrapper|PFDataOperatorState|PFEngine|PFIntegrator|PFPlaneMeshWrapper|PFSimpleActionState|PFSphereMeshWrapper|PFSystemPool|PMAlpha|POmniFlect|POmniFlectMod|Paint|PainterInterface|PalmTrans|ParamBlockParamBlock|ParameterCollectorCA|ParameterEditor|ParserLoader|ParticleChannelBool|ParticleChannelFloat|ParticleChannelID|ParticleChannelINode|ParticleChannelInt|ParticleChannelMap|ParticleChannelMesh|ParticleChannelNew|ParticleChannelPTV|ParticleChannelPair|ParticleChannelQuat|ParticleChannelSel|ParticleChannelTest|ParticleContainer|ParticleGroup|Particles|PatchDeform|PathDeform|PathDeformSpaceWarp|PersistentNodeSet|PhilBitmap|Phong|PhysXPanel|PhysXShapeConvex|Physical|PhysicalMaterial|Physique|Pipe|PipeReferenceTarget|Pivoted|Plane|Planet|Plant|PlantImporterPlugin|Point|PointCloud|PolyMeshObject|PolyToolsModeling|PolyToolsPaintDeform|PolyToolsPolyDraw|PolyToolsSelect|PolyToolsShift|PolyToolsTopology|PolyToolsUVWTweak|PolymorphicGeom|PopCharacter|PopRefUtil|PopSegControl|PopSkinControl|PopSkinObject|Populate|PostSceneFragment|PreSceneFragment|Preserve|PresetDummyOperIcon|PresetDummyOperator|PresetDummyTest|PresetOperIcon|PresetOperator|PresetTest|PresetTestIcon|Priority|Prism|ProBoolean|ProCutter|Projection|Properties|Properties|Properties|Protractor|ProxSensor|Push|PushMod|PushSpaceWarp|Pyramid|PythonHost|QTime|Quadratic|QuarterRound|RLA|RPF|Radiosity|Railing|RandomColor|RayFX|RayMeshGridIntersect|Raytrace|RaytraceMaterial|Rectangle|RefTargContainer|RefTargMonitor|Reflection|ReflectionFragment|Refraction|Relax|RendPlane|RenderEffects|RenderElementMgr|RenderEnvironment|RenderParticles|Reservoir|Retimer|RetimerMasterCtrl|RingWave|Ripple|Ripplebinding|RmModel|RmModelGeometry|SATImport|SDeflectMod|SDeflector|SOmniFlect|SOmniFlectMod|Scalar|ScaleParticles|ScaleXYZ|Scatter|Seat|SelectionFragment|ShadowRenderElement|ShadowsMap|ShapeControl|ShapeLibrary|ShapeMap|ShapeMerge|ShapeStandard|SharedMotionFlow|SharedMotionFlows|ShaveStylinIcon|Shell|Shellac|ShineExp|SimpleFaceData|SimpleOSMToWSMMod|SimpleOSMToWSMMod2|Skeleton|Skew|Skin|SkinUtilities|Skylight|SlaveFloat|SlavePos|SlaveRotation|SlaveScale|SliceModifier|SlidingDoor|SlidingWindow|Smoke|Snow|SoftEdge|Soften|SortKey|Sound|SpaceBend|SpaceCameraMap|SpaceConform|SpaceFFDBox|SpaceFFDCyl|SpaceNoise|SpacePatchDeform|SpacePathDeform|SpaceSkew|SpaceStretch|SpaceSurfDeform|SpaceTaper|SpaceTwist|Spacedisplace|Spaceripple|Spacewave|Spawn|Speckle|Specular|Sphere|SphereGizmo|Spherify|SpinLimit|Spindle|Splat|SplineSelect|SplineShape|Spray|Spring|Squeeze|StandardUVGen|StandardXYZGen|Standardmaterial|Star|Stars|Strauss|Stretch|Strokes|Stucco|Substitute|Sunlight|SurfDeform|Switch|TIF|Tape|Taper|Targa|Targetcamera|Targetobject|Teapot|Tee|TempObjectClass|Terrain|TextMap|TextPlus|TextureObjMask|TimeSensor|ToneMappingFragment|TooltipFragment|TopBottom|Torus|TouchSensor|TriMeshGeometry|TrueType|Tube|TurboSmooth|Twist|Utility|Uvwmap|VRImgIO|Vector|VertexPaint|Video|ViewportManager|Vortex|VortexMod|VrmlImp|VzMaterialTable|WalkThroughGUP|Wall|WalledRectangle|Water|Wave|WaveMaster|WaveObject|Wavebinding|Welder|WideFlange|Wind|Windbinding|Wipe|WireframeFragment|Wood|WrapperList|XForm|XRefObject|add|alpha|bipedSystem|blur|channel|clamp|collapse|colorReferenceTarget|compositematerial|contrast|cubic|curve|dSightRender|decay|diffuseMap|distort|falloff|fallofftextureMap|freeSpot|gravity|grid|icon|line|lookat|mCloth|mapping|mappinglayerDataDef|materialhelper|meshsmooth|mirror|negative|nvBox|nvCapsule|nvRagdoll|offsetTimer|optimize|output|parameter|pivot|projected|quadPatch|radiusManip|raytraceShadow|resizer|rgb|rotation|section|shadowMap|sliderManipulator|smooth|specularMap|speed|spin|stop|subdivide|surface|sweep|swirl|symmetry|targetSpot|tessellate|text|triPatch|velocity|xView)\\b"
				},
